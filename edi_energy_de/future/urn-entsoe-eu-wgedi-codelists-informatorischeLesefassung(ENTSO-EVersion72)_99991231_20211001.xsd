        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="B20">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Status request for a reporting information market document</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">A document requesting the provision of a reporting information document.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="B21">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Reserve need document</xsd:documentation>
