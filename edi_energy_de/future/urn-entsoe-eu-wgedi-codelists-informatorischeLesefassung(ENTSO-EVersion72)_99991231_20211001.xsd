        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="B46">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Resource capacity unit document</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">A document providing information about resource capacity units.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
