        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="B28">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Move of scheduled production</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">A document indication a move of scheduled production.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="B29">
