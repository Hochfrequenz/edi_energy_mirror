        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A36">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Planned</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">Is planned.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A37">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Confirmed</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">The status is  confirmed.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A38">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Shall Be Used</xsd:documentation>
