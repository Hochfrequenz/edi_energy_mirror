          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">kilowatt hour</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">A total amount of electrical energy transferred or consumed in one hour.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="KWT">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">kilowatt</xsd:documentation>
