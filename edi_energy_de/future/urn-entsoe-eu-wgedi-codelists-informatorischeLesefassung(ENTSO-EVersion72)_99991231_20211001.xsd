  <xsd:simpleType name="StandardIndicatorTypeList">
    <xsd:annotation>
      <xsd:documentation source="Uid">ET0029</xsd:documentation>
      <xsd:documentation source="Definition">A boolean indicator to express Yes or No or True or False.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:NMTOKEN">
      <xsd:enumeration value="A01">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">YES</xsd:documentation>
