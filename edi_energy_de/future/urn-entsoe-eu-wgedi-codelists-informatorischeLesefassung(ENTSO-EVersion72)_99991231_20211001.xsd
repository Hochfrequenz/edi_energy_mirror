      <xsd:enumeration value="A99">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">HVDC Link constraints</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">A document providing the information concerning the maximum and minimum active power flow through each link can limited.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="B01">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">HVDC Configuration</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">A document providing the information concerning the power set point. The mode in which the  HVDC is managed.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="B02">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">HVDC Schedule</xsd:documentation>
