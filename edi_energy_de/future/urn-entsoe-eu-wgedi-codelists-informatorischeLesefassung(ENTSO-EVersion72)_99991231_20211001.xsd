      </xsd:enumeration>
      <xsd:enumeration value="B38">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Settlement document</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">A document providing settlement information.</xsd:documentation>
        </xsd:annotation>
