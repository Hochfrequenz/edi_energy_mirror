          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Hertz</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">HTZ unit as per UN/CEFACT recommendation 20.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="KEL">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">K (Kelvin)</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">Temperature unit refer ISO 80000-5 (Quantities and units, Part 5: Thermodynamics).</xsd:documentation>
