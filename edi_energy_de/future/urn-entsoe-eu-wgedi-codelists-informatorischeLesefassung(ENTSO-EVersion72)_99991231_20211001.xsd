        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">HVDC Configuration</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">A document providing the information concerning the power set point. The mode in which the  HVDC is managed.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="B02">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">HVDC Schedule</xsd:documentation>
