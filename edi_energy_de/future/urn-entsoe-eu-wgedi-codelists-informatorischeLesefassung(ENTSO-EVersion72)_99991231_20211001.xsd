        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A89">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Contracted reserve prices</xsd:documentation>
