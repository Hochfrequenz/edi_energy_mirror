      </xsd:enumeration>
      <xsd:enumeration value="B35">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Area Configuration document</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">A document containing the definition of areas.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="B36">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
