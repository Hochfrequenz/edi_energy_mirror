        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A39">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Could Be Used</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">The object defined in the series could be used.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A40">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Proposed</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">The status of the information is proposed.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A41">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Individual Network Data</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">The network data provided in the document or series concerns the the unique TSO area describes by the document or series.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A42">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Common Network Data</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">The network data provided in the document or series concerns the whole area describes by the document or series.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A43">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
