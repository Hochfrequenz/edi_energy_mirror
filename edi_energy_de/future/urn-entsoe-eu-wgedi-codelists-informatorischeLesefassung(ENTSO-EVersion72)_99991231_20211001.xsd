        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A02">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Metering point</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">The object being described concerns a metering point.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A03">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Party</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">The object being described concerns a party.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A04">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Agreement Identification</xsd:documentation>
