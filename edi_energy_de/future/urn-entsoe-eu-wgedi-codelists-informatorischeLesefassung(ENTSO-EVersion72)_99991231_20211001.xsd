        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="B03">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">EIC code request</xsd:documentation>
