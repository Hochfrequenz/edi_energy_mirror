      <xsd:enumeration value="B39">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Imbalance prognosis document</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">A document to provide the prognosis of energy imbalances for a given period.</xsd:documentation>
