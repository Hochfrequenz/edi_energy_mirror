      <xsd:enumeration value="A95">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Configuration document</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">A document providing configuration information for the network.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A96">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Redispatch activation document</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">A document enabling the activation of a redispatch notice.</xsd:documentation>
        </xsd:annotation>
