        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Flow based domain</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">A document providing the capacity domain and its limits available for the TSO.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="B09">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Flow based domain publication</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">A document providing the capacity domain and its limits available for the market.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="B10">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Flow based domain market impact publication</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">A document providing the capacity domain and its impacts on the market to be published.</xsd:documentation>
        </xsd:annotation>
