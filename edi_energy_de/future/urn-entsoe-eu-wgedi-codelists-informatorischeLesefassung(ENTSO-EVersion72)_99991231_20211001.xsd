  <xsd:simpleType name="StandardRightsTypeList">
    <xsd:annotation>
      <xsd:documentation source="Uid">ET0938</xsd:documentation>
      <xsd:documentation source="Definition">The rights of use that is accorded to what is acquired in an auction.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:NMTOKEN">
      <xsd:enumeration value="A01">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Use It Or Lose It</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">Any rights not nominated shall be lost.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A02">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Use It Or Sell It</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">Any rights that are not nominated shall be sold.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A03">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Allocation curtailment possible</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">Rights acquired may be curtailed.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A04">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Nomination curtailment possible</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">Rights acquired may be curtailed at nomination.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A05">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
