      <xsd:enumeration value="B11">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Anonymized flow based parameters publication</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">A document providing all the relevant flow based parameters in case of flow based capacity allocation.</xsd:documentation>
