    <xsd:restriction base="xsd:NMTOKEN">
      <xsd:enumeration value="A01">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Tieline</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">A high voltage line used for cross border energy interconnections.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A02">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Line</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">A specific electric line within a country.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A03">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Resource Object</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">A resource that can either produce or consume energy.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A04">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Generation</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">A resource that can produce energy.</xsd:documentation>
