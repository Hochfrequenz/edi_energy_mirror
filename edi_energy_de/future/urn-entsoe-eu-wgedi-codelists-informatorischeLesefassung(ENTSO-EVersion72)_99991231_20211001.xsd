        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A05">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Accounting point</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">The object being described concerns an accounting point.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A06">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Resource Object</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">The object being described concerns a resource object.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A07">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Tieline</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">The object being described concerns a tieline.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A08">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Resource type</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">The object being described concerns a resource type.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A09">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">DC link</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">The object being described concerns a DC link.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A10">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">AC link</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">The object being described concerns an AC link.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A11">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Merchant line</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">The object being described concerns a merchant line.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A12">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Bidding zone</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">The largest geographical area within which market participants are able to exchange energy without capacity allocation.</xsd:documentation>
