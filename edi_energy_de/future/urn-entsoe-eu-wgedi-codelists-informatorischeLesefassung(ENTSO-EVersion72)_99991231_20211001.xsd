      </xsd:enumeration>
      <xsd:enumeration value="A05">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Load</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">A resource that can consume energy.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A06">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Phase Shift Transformer</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">An electrical device for controlling the power flow through specific lines in a power transmission network.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A07">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Circuit Breaker</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">An electrical switch designed to protect an electrical circuit from damage caused by overcurrent/overload or short circuit.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A08">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Busbar</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">A specific element within a substation to connect grid elements for energy distribution purposes.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A09">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Capacitor</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">A transmission element designed to inject reactive power into the transmission network.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A10">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Inductor</xsd:documentation>
