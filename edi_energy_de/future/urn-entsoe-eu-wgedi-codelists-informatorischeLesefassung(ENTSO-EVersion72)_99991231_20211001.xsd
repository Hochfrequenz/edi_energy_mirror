          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">Megawatt per Hertz</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">A unit of energy expressed as the load change in million watts that will cause a frequency shift of one hertz.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="GWH">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">gigawatt hour</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">GWh unit as per UN/CEFACT recommendation 20.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="HMQ">
        <xsd:annotation>
          <xsd:documentation source="CodeDescription"/>
          <xsd:documentation source="CodeDescription/Title">cubic hectometres</xsd:documentation>
          <xsd:documentation source="CodeDescription/Definition">A unit of volume equal to one million cubic metres.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="HTZ">
